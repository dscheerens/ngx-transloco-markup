{
  "name": "ngx-transloco-markup",
  "version": "3.1.0",
  "description": "A translation component to render translations with markup",
  "homepage": "https://github.com/dscheerens/ngx-transloco-markup",
  "author": "Daan Scheerens",
  "license": "MIT",
  "keywords": [
    "angular",
    "ngx",
    "transloco",
    "markup",
    "plugin",
    "translation",
    "translate",
    "i18n"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dscheerens/ngx-transloco-markup"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=demo",
    "serve.prod": "ng serve --project=demo --configuration production",
    "lint": "tslint -p lib/tsconfig.lib.base.json -t stylish -c lib/tslint.json",
    "test": "ng test --project=lib --watch=false --code-coverage",
    "test.watch": "ng test --project=lib --watch=true --code-coverage --browsers=Chrome",
    "build": "ng build --project=lib --configuration production",
    "build.demo": "ng build --project=demo --configuration production",
    "release": "standard-version --message \"release: v%s\"",
    "publish.dist": "npm publish build/dist"
  },
  "peerDependencies": {
    "@angular/common": ">=13.0.0",
    "@angular/core": ">=13.0.0",
    "@ngneat/transloco": "^3 || ^4",
    "rxjs": "^6 || ^7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.5",
    "@angular/animations": "^13.3.8",
    "@angular/cdk": "^13.3.7",
    "@angular/cli": "^13.3.5",
    "@angular/common": "^13.3.8",
    "@angular/compiler": "^13.3.8",
    "@angular/compiler-cli": "^13.3.8",
    "@angular/core": "^13.3.8",
    "@angular/forms": "^13.3.8",
    "@angular/material": "^13.3.7",
    "@angular/platform-browser": "^13.3.8",
    "@angular/platform-browser-dynamic": "^13.3.8",
    "@angular/router": "^13.3.8",
    "@dscheerens/tslint-presets": "^9.0.0",
    "@ngneat/spectator": "^10.0.1",
    "@ngneat/transloco": "^4.0.0",
    "@types/jasmine": "~3.10.2",
    "@types/jasminewd2": "~2.0.10",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^3.10.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^4.0.1",
    "ng-packagr": "^13.2.1",
    "ngx-typesafe-forms": "^1.5.2",
    "rxjs": "^6.6.2",
    "standard-version": "^9.3.2",
    "tslib": "^2.3.0",
    "tslint": "~6.1.3",
    "typescript": "~4.5.5",
    "typescript-tslint-plugin": "^1.0.1",
    "zone.js": "~0.11.4"
  }
}
