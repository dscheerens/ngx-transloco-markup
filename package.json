{
  "name": "ngx-transloco-markup",
  "version": "5.1.0",
  "description": "A translation component to render translations with markup",
  "homepage": "https://github.com/dscheerens/ngx-transloco-markup",
  "author": "Daan Scheerens",
  "license": "MIT",
  "keywords": [
    "angular",
    "ngx",
    "transloco",
    "markup",
    "plugin",
    "translation",
    "translate",
    "i18n"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dscheerens/ngx-transloco-markup"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=demo",
    "serve.prod": "ng serve --project=demo --configuration production",
    "lint": "ng lint --project=lib",
    "test": "ng test --project=lib --watch=false --code-coverage",
    "test.watch": "ng test --project=lib --watch=true --code-coverage --browsers=Chrome",
    "build": "ng build --project=lib --configuration production",
    "build.demo": "ng build --project=demo --configuration production",
    "release": "standard-version --message \"release: v%s\"",
    "publish.dist": "npm publish build/dist"
  },
  "peerDependencies": {
    "@ngneat/transloco": "^5 || ^6",
    "rxjs": "^7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.8",
    "@angular-eslint/builder": "^16.1.0",
    "@angular-eslint/eslint-plugin": "^16.1.0",
    "@angular-eslint/eslint-plugin-template": "^16.1.0",
    "@angular-eslint/template-parser": "^16.1.0",
    "@angular/animations": "^16.1.8",
    "@angular/cdk": "^16.1.7",
    "@angular/cli": "^16.1.8",
    "@angular/common": "^16.1.8",
    "@angular/compiler": "^16.1.8",
    "@angular/compiler-cli": "^16.1.8",
    "@angular/core": "^16.1.8",
    "@angular/forms": "^16.1.8",
    "@angular/material": "^16.1.7",
    "@angular/platform-browser": "^16.1.8",
    "@angular/platform-browser-dynamic": "^16.1.8",
    "@angular/router": "^16.1.8",
    "@dscheerens/eslint-config-angular": "^3.0.0",
    "@ngneat/spectator": "^15.0.1",
    "@ngneat/transloco": "^6.0.4",
    "@types/jasmine": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "jasmine-core": "^4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "ng-packagr": "^16.1.0",
    "rxjs": "^7.8.0",
    "standard-version": "^9.5.0",
    "typescript": "~4.9.5",
    "zone.js": "~0.13.1"
